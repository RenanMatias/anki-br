{
  "name": "my-pocket",
  "version": "1.0.0",
  "description": "Um local onde você controla suas finanças e seus investimentos.",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "docker:up": "sudo docker compose -f infra/compose.yaml up -d",
    "docker:down": "sudo docker compose -f infra/compose.yaml down",
    "docker:stop": "sudo docker compose -f infra/compose.yaml stop",
    "docker:list": "sudo docker ps",
    "docker:psql": "psql --host=localhost --username=postgres --port=5434"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^2.8.8"
  }
}
